      <definition value="Personlig hjælp og pleje"/>
      <concept>
        <code value="H1.1"/>
        <display value="Personlig hygiejne"/>
        <definition value="Alle indsatser relateret til den personlige hygiejne, som fx bad, hårvask, øvre og nedre toilette, af- og påklædning og negleklipning. Inkluderer også af- og påsætning af kropsbårne hjælpemidler (der ikke er omfattet af en sundhedslovsydelse)."/>
      </concept>
      <concept>
        <code value="H1.2"/>
        <display value="Udskillelser"/>
        <definition value="Alle indsatser relateret til det at gå på toilettet, hvad enten borgeren selv kan, har brug for assistance eller for en erstatning for toiletbesøg i form af bleskift, uridomskift eller bækken-/kolbetømning."/>
      </concept>
      <concept>
        <code value="H1.3"/>
        <display value="Ernæring"/>
        <definition value="Aktiviteter der foregår fra bord til mund. Altså alle indsatser, der vedrører indtagelse af mad og drikke, hvad enten der er tale om indsats til den egentlig handling at spise eller en indsats, der sikrer samvær under måltidet."/>
      </concept>
      <concept>
        <code value="H1.4"/>
        <display value="Mobilitet"/>
        <definition value="Alle aktiviteter i forbindelse med forflytning, lejring og vending."/>
      </concept>
      <concept>
        <code value="H1.5"/>
        <display value="Hverdagens aktiviteter"/>
        <definition value="Alle administrative og strukturelle opgaver. Det gælder både brevskrivning, hjælp til struktur, hjælp til medicin, hvor der ikke er tale om en overleveret ydelse, men fordi borgeren ikke selv har fysik til fx at håndtere sine piller, kontakt til myndighedspersoner, familie og venner."/>
      </concept>
      <concept>
        <code value="H1.6"/>
        <display value="Tilsyn/omsorg"/>
        <definition value="Besøg der ikke er forbundet med en konkret opgave, men handler om at give en psykisk støtte, tryghed hos borgeren."/>
      </concept>
    </concept>
    <concept>
      <code value="H1b"/>
      <display value="Hjemmehjælp - Praktisk hjælp og støtte"/>
      <definition value="Hjælp eller støtte til nødvendige praktiske opgaver i hjemmet."/>
      <concept>
        <code value="H1.8"/>
        <display value="Rengøring"/>
        <definition value="Alle dele af rengøring, både den daglige og hovedrengøring. Bemærk: Skift af sengelinned samt evt. snerydning er en rengøringsydelse. Bækken-/kolbetømning kan også placeres her, såfremt det ikke indgår i forbindelse med en personlig pleje-indsats."/>
      </concept>
      <concept>
        <code value="H1.9"/>
        <display value="Tøjvask"/>
        <definition value="Alle former for håndtering af vasketøj - fra snavsetøjskurven til tøjskabet."/>
      </concept>
      <concept>
        <code value="H1.7"/>
        <display value="Indkøb"/>
        <definition value="Aktiviteter foretaget for at skaffe dagligvarer - fra indkøbsseddel skrives, til varer er stillet på plads."/>
      </concept>
      <concept>
        <code value="H1.12"/>
        <display value="Praktiske indsatser i relation til børn i husstanden"/>
        <definition value="Tilbagevendende praktiske opgaver der relaterer sig til børn i husstanden. Indsatsen bevilliges som praktisk bistand til den forælder, der ikke selv er i stand til at udføre opgaven."/>
      </concept>
      <concept>
        <code value="H1.10"/>
        <display value="Tilberede/anrette mad"/>
        <definition value="Aktiviteter der foregår fra køkken til bord - den praktiske del af måltidet."/>
      </concept>
    </concept>
    <concept>
      <code value="H1c"/>
      <display value="Madservice"/>
      <definition value="Der leveres mad til hjemmet."/>
      <concept>
