       ] [
         fhir:code [ fhir:v "G1.31" ] ;
         fhir:display [ fhir:v "Samarbejde med netværk" ] ;
         fhir:definition [ fhir:v "Indsatsen består i samarbejde med pårørende om de indsatser, der ydes til borgeren, fx støtte til pårørende til en borger med demens eller psykisk sygdom." ]
       ] [
