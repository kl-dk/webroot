       ] [
         fhir:code [ fhir:v "e4279057-8349-499e-8ae3-6271aac55c2b" ] ;
         fhir:display [ fhir:v "Udfordring i forældre-barn-relation" ] ;
         fhir:definition [ fhir:v "Udfordring i forældre-barn-relation" ] ;
         fhir:concept ( [
           fhir:code [ fhir:v "00a9f897-f784-4164-966c-eba94171ec82" ] ;
           fhir:display [ fhir:v "Uforudsigelighed" ] ;
           fhir:definition [ fhir:v "Småbørn: Forældrenes forhold til barnet bærer præg af uforudsigelighed. Dette kan både give sig til udtryk i manglende rutiner i løbet af døgnet og i forældrenes manglende evne til at skabe struktur og genkendelighed i forhold til fx søvn, mad, kontakt, aktivitet og omsorg. Det kan også give sig udtryk i at barnet fx nogle gange trøstes med det samme og andre gange får lov at græde." ]
         ] [
           fhir:code [ fhir:v "51bc227a-3455-474d-bf42-fc7a7875513c" ] ;
           fhir:display [ fhir:v "Følelsesmæssige behov opfyldes ikke" ] ;
           fhir:definition [ fhir:v "Småbørn: Barnets følelsesmæssige behov opfyldes ikke tilstrækkeligt. Fx trøstes, holdes eller kontaktes barnet ikke tilstrækkeligt eller barnets behov for pauser mødes ikke (overstimulation).\n\nSkolebørn: Barnets følelsesmæssige behov opfyldes ikke af forældrene fx indgår de ikke i kontakt med barnet i tilstrækkeligt omfang, trøster det ikke tilstrækkeligt og støtter ikke barnet tilstrækkeligt i følelser som vrede, skam, glæde, stolthed, nysgerrighed, frygt eller ængstelse." ]
         ] [
           fhir:code [ fhir:v "53db2598-28be-4d04-8b3a-0005581f1fda" ] ;
           fhir:display [ fhir:v "Forældreudfordring ved særlige behov" ] ;
           fhir:definition [ fhir:v "Småbørn: Forældrene mangler viden og/eller overskud til at reagere på barnets særlige behov. Det, der registreres her, er altså IKKE, at barnet har særlige behov. Det registreres, hvis barnets særlige behov er en medvirkende forklaring på, at barnets behov ikke mødes. Det kan fx være et præmaturt barn, der spiser meget ofte og er meget grædende, og hvor forældrenes manglende overskud, viden og forståelse kan føre til momenter med manglende omsorg.\n\nSkolebørn: Barnet har særlige behov, og forældrene mangler viden og/eller overskud. Det der registreres her, er altså IKKE, at barnet har særlige behov. Det registreres hvis barnets særlige behov er en medvirkende forklaring på, at barnets behov ikke mødes." ]
         ] [
           fhir:code [ fhir:v "a038c064-b57d-4a62-9333-c9456817595d" ] ;
           fhir:display [ fhir:v "Fysiske behov opfyldes ikke" ] ;
           fhir:definition [ fhir:v "Småbørn: Barnets fysiske behov opfyldes ikke tilstrækkeligt. Fx kan der være udfordringer med hygiejnisk pleje af barnet, passende ernæring, påklædning i forhold til omgivelser, tilpas fysisk stimulation, indeklima m.m.\n\nSkolebørn: Barnets fysiske behov opfyldes ikke tilstrækkeligt fx kan der være udfordringer med soignering og fysiske fornødenheder." ]
         ] [
           fhir:code [ fhir:v "a701d676-dfe5-49fc-862c-92befd64739a" ] ;
           fhir:display [ fhir:v "Konflikter i forældre-barn samspil" ] ;
           fhir:definition [ fhir:v "Skolebørn: Samspil mellem barnet og forældre er præget af konflikter fx mange skænderier." ]
         ] [
           fhir:code [ fhir:v "d3fc57b7-3a5d-4f69-bcb0-bdb8363650eb" ] ;
           fhir:display [ fhir:v "Udfordring i forældre-barn samspil" ] ;
           fhir:definition [ fhir:v "Småbørn: Barnet viser utryg tilknytning. Fx foretrækker ikke forældre, eller er ikke interesseret i kontakt med dem. Opleves meget ked af det og urolig eller passivt, tilbagetrukket og/eller uden mimik i samspillet med forældre" ]
         ] [
           fhir:code [ fhir:v "fc4b6e6d-a055-49bf-adab-474e0a810675" ] ;
           fhir:display [ fhir:v "Aktiviteter ikke afpasset" ] ;
           fhir:definition [ fhir:v "Småbørn: Aktiviteter afpasses ikke efter barnets behov. Fx for få, for mange eller for forskellige aktiviteter i løbet af dagene. Kan også være manglende hensyn i forhold til barnets tilstand og aktivitet." ]
         ] )
       ] [
