<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".html" />
      <mimeMap fileExtension=".html" mimeType="text/html;charset=UTF-8" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="children.2.0.1.Questionnaire">
          <match url="^(Questionnaire)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crquestionnaire.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.ImplementationGuide">
          <match url="^(ImplementationGuide)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crimplementationguide.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.Bundle">
          <match url="^(Bundle)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crbundle.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.Patient">
          <match url="^(Patient)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crpatient.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.QuestionnaireResponse">
          <match url="^(QuestionnaireResponse)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crquestionnaireresponse.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.CodeSystem">
          <match url="^(CodeSystem)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcodesystem.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.RelatedPerson">
          <match url="^(RelatedPerson)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crrelatedperson.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.StructureDefinition">
          <match url="^(StructureDefinition)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crstructuredefinition.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.CarePlan">
          <match url="^(CarePlan)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcareplan.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.Observation">
          <match url="^(Observation)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crobservation.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.Encounter">
          <match url="^(Encounter)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crencounter.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.ValueSet">
          <match url="^(ValueSet)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crvalueset.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="children.2.0.1.Location">
          <match url="^(Location)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crlocation.asp?type={R:1}&amp;id={R:2}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>