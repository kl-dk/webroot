<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".html" />
      <mimeMap fileExtension=".html" mimeType="text/html;charset=UTF-8" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="core.1.2.0.Condition">
          <match url="^(Condition)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcondition.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Provenance">
          <match url="^(Provenance)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crprovenance.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Organization">
          <match url="^(Organization)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crorganization.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.CareTeam">
          <match url="^(CareTeam)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcareteam.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.ImplementationGuide">
          <match url="^(ImplementationGuide)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crimplementationguide.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Patient">
          <match url="^(Patient)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crpatient.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.ServiceRequest">
          <match url="^(ServiceRequest)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crservicerequest.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.RelatedPerson">
          <match url="^(RelatedPerson)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crrelatedperson.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Practitioner">
          <match url="^(Practitioner)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crpractitioner.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.CarePlan">
          <match url="^(CarePlan)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcareplan.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.StructureDefinition">
          <match url="^(StructureDefinition)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crstructuredefinition.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Goal">
          <match url="^(Goal)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crgoal.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.Observation">
          <match url="^(Observation)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crobservation.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="core.1.2.0.ClinicalImpression">
