<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".html" />
      <mimeMap fileExtension=".html" mimeType="text/html;charset=UTF-8" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="gateway.1.1.0.Condition">
          <match url="^(Condition)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcondition.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.ImplementationGuide">
          <match url="^(ImplementationGuide)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crimplementationguide.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Bundle">
          <match url="^(Bundle)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crbundle.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Patient">
          <match url="^(Patient)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crpatient.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.CodeSystem">
          <match url="^(CodeSystem)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcodesystem.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.StructureDefinition">
          <match url="^(StructureDefinition)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crstructuredefinition.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.CarePlan">
          <match url="^(CarePlan)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcareplan.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Goal">
          <match url="^(Goal)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crgoal.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Observation">
          <match url="^(Observation)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crobservation.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.CapabilityStatement">
          <match url="^(CapabilityStatement)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crcapabilitystatement.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Encounter">
          <match url="^(Encounter)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crencounter.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.Procedure">
          <match url="^(Procedure)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crprocedure.asp?type={R:1}&amp;id={R:2}" />
        </rule>
        <rule name="gateway.1.1.0.ValueSet">
          <match url="^(ValueSet)/([A-Za-z0-9\-\.]{1,64})" />
          <action type="Rewrite" url="crvalueset.asp?type={R:1}&amp;id={R:2}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>